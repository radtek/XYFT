@{
    ViewBag.Title = "recharge";
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
<head>

    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>充值记录</title>
    <link rel="stylesheet" type="text/css" href="/newstatic/css/reset.css" />
    <link rel="stylesheet" type="text/css" href="/newstatic/css/common.css" />
    <link rel="stylesheet" type="text/css" href="/newstatic/css/back.css" />
    <link rel="stylesheet" type="text/css" href="/newstatic/css/footer.css" />
    <script src="~/newstatic/js/jquery-1.11.3.min.js"></script>
    <style type="text/css">
        body {
            background: #F0F0F0;
        }
    </style>
</head>
<body>
    <header>
        <h2><span onclick="history.go(-1)"><i class="icon"></i></span>充值记录</h2>
    </header>
    <div class="headblank"></div>
    <div class="ch-head">
        <table class="cha">
            <tr>
                <th>充值金额</th>
                <th>变动时间</th>
                <th>充值状态</th>
            </tr>
        </table>
    </div>
    <div class="main table">
        <table class="cha">
            <tbody>
                <foreach name="list" item="vo">
                    <tr>
                        <td>300</td>
                        <td>04-21 02:00</td>
                        <td>
                            <!--<if condition="$vo['Type'] eq 1">充值处理
                            <elseif condition="$vo['Type'] eq 2"/>充值成功
                            <else />-->
                            充值成功
                            </if>
                        </td>
                    </tr>
                </foreach>
                <tr>
                    <td colspan="3"></td>
                </tr>
            </tbody>
        </table>


    </div>
    <div class="footblank"></div>
    <!-- 底部导航 -->
    @Html.Partial("_footer")
    <!-- 底部导航 -->
    
</body>
</html>

