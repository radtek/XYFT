@model OWZX.Web.Models.SafeVerifyModel
@{
    Layout = null;
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML>
    <HEAD>

        <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no" />
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

        <link rel="stylesheet" type="text/css" href="~/newstatic/css/reset.css" />
        <link rel="stylesheet" type="text/css" href="~/newstatic/css/common.css" />
        <link rel="stylesheet" type="text/css" href="~/newstatic/css/back.css" />
        <link rel="stylesheet" type="text/css" href="~/newstatic/css/footer.css" />
        <style>
            body {
                background: #F0F0F0;
            }

            .ico3 {
                display: inline-block;
                height: 2em;
                width: 2em;
                background: url(/newstatic/img/ico3.jpg) no-repeat;
                background-size: cover;
            }

            .wallet .head {
                color: #fff;
                font-size: 1.2em;
            }

                .wallet .head div {
                    display: inline-block;
                    color: #fff;
                    line-height: 1.5em;
                    margin-top: 1em;
                }

                .wallet .head img {
                    width: 3em;
                    height: 3em;
                    font-size: 1.5em;
                    display: inline-block;
                    vertical-align: top;
                }

            .wallet .wamenu {
                padding: .9em .7em;
            }

                .wallet .wamenu li {
                    font-size: 1.1em;
                    height: 3em;
                    line-height: 3em;
                    background: #fff;
                    margin-bottom: .5em;
                    padding: 0.2em 1em;
                    font-weight: 500;
                    border-radius: .4em;
                }

                    .wallet .wamenu li i {
                        float: left;
                        margin: .4em 1em 0 .5em;
                    }

                    .wallet .wamenu li:nth-child(1) i {
                        background-position: 0 -16em;
                        background-size: cover;
                    }

                    .wallet .wamenu li:nth-child(2) i {
                        background-position: 0 -26em;
                        background-size: cover;
                    }

                    .wallet .wamenu li:nth-child(3) i {
                        background-position: 0 -28em;
                        background-size: cover;
                    }

                    .wallet .wamenu li:nth-child(4) i {
                        background-position: 0 -30em;
                        background-size: cover;
                    }

                    .wallet .wamenu li:nth-child(5) i {
                        background-position: 0 -34em;
                        background-size: cover;
                    }

            .tishi {
                width: 80%;
                position: fixed;
                top: 100%;
                left: 10%;
                background: #fff;
                z-index: 999;
                border-radius: 5px;
                text-align: center;
                padding-bottom: 1em;
            }

                .tishi .ts {
                    line-height: 2em;
                    border-bottom: #eee solid 1px;
                    font-size: 1.2em;
                    font-weight: 700;
                }

                .tishi .tsnr {
                    line-height: 2em;
                    margin: 1em .5em;
                }
        </style>
    </HEAD>

    <BODY>
        <header>
            <h2><span onclick="history.go(-1)"><i class="icon"></i></span>设置</h2>
        </header>
        <div class="headblank"></div>
        <div class="wallet">

            <ul class="wamenu">
                <li onclick="window.location.href='unionpay.html'"><i class="ico3"></i> <a>银行卡列表</a></li>
                <li onclick="window.location.href='pwd.html'"><i class="ico3"></i> <a>修改密码</a></li>
                <li onclick="window.location.href='carrypwd.html'"><i class="ico3"></i> <a>提现密码</a></li>
                <li onclick="window.location.href='tel.html'"><i class="ico3"></i> <a>手机绑定</a></li>
                <li onclick="window.location.href = '/Account/Logout'"><i class="ico3"></i> <a>退出</a></li>
            </ul>
        </div>
        <div class="footblank"></div>
        <!-- 底部导航 -->

        @Html.Partial("_footer")
        <!-- 底部导航 -->
        <!-- 提示框 -->
        <div class="betbg"></div>
        <div class="tishi">
            <div class="ts">提示</div>
            <div class="tsnr"></div>
            <div class="btn" style="width:80%;">确定</div>
        </div>
        <script src="~/newstatic/js/jquery-1.11.3.min.js"></script>
        <script type="text/javascript">
              $(".tishi .btn").click(function(){
              $(".betbg").fadeOut(100);
              $(".tishi").animate({top:"100%",marginTop:"0"},300);
              });
              function tishi(nr){
	              $(".betbg").fadeIn(100);
	              $(".tsnr").html(nr);
	              $(".tishi").animate({top:"50%",marginTop:"-3.5em"},300);
             };


            function jump(href)
            {
              window.location.href="/Home"+href;
            }
        </script>
     


</body>
</html>